{
  "name": "vuejs-slack-clone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon ./bin/www",
    "dev:client": "cd client && npm run serve",
    "dev": "concurrently --kill-others-on-fail 'npm run dev:server' 'npm run dev:client'",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "http": "0.0.1-security",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "postcss": "^7.0.35",
    "socket.io": "^3.0.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
